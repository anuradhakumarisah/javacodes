import java.util.Scanner;
public class largsmall
{
	public static void main(String args[])
	{
		int l=-1,s=0,n,i;
		Scanner sc=new Scanner(System.in);
		for(i=1;i<=5;i++){
			n=sc.nextInt();
			if(i==1){
				l=n;
				s=n;
			}
			else if(i>1){
				if(n>l)
					l=n;
				else if(n<s)
					s=n;
					
			}
				
		}
		System.out.print("large = "+l+"  small = "+s);
	}
}
