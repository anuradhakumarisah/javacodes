public class NP15
{
	public static void main(String args[])
	{
		int i,j,k,c;
		for(i=1;i<=5;i++){
			c=i-1;
			for(j=1;j<=5;j++){
				if(j>=i){
					System.out.print(j);
				}
			}
			for(k=1;k<=5;k++){
				if(k>(6-i)){
					System.out.print(c);
					c--;	
				}
			
			}
			System.out.println();
		}
	}
}
